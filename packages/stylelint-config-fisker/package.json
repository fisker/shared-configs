{
  "name": "stylelint-config-fisker",
  "version": "0.0.4",
  "description": "fisker's Shareable stylelint Configuration",
  "main": "index.js",
  "keywords": ["fisker", "stylelint", "config"],
  "files": ["index.js"],
  "scripts": {
    "release": "np --no-yarn --no-cleanup",
    "prettier": "prettier **/*.{js,md,json} --write",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint **/*.js",
    "lint:markdown": "markdownlint *.md"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "fisker/stylelint-config-fisker",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-fisker": "^4.3.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "prettier": "*",
    "stylelint": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "bugs": {
    "url": "https://github.com/fisker/stylelint-config-fisker/issues"
  },
  "homepage": "https://github.com/fisker/stylelint-config-fisker#readme",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "dependencies": {
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4"
  }
}
