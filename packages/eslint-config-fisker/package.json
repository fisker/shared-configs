{
  "name": "@xwtec/eslint-config",
  "version": "3.7.0",
  "description": "ESLint Configuration for xwtec projects",
  "main": "index.js",
  "keywords": ["xwtec", "eslint", "config"],
  "files": [
    "index.js",
    "legacy.js",
    "vue.js",
    "configs/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "release": "np --no-yarn --no-cleanup",
    "test": "echo passed.",
    "prettier": "prettier **/*.{js,md,json} --write",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint **/*.js",
    "lint:markdown": "markdownlint *.md"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-unicorn": "^7.1.0",
    "eslint-plugin-vue": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xwtec/eslint-config.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@xwtec/eslint-config": "^3.7.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.15.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.14.0",
    "np": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "bugs": {
    "url": "https://github.com/xwtec/eslint-config/issues"
  },
  "homepage": "https://github.com/xwtec/eslint-config#readme"
}
