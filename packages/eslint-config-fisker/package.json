{
  "name": "eslint-config-fisker",
  "version": "4.3.4",
  "description": "fisker's Shareable ESLint Configuration",
  "main": "index.js",
  "keywords": ["fisker", "eslint", "config"],
  "files": ["index.js", "legacy.js", "vue.js", "configs/"],
  "scripts": {
    "release": "run-s lint build prettier test release:*",
    "release:npm": "np --no-yarn --yolo",
    "build": "npm-run-all build:*",
    "build:compare": "node -r esm scripts/compare.js",
    "prettier": "prettier **/*.{js,md,json} --write",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint **/*.js --fix",
    "lint:markdown": "markdownlint *.md",
    "test": "npm-run-all test:*",
    "test:js": "eslint **/*.js --config ./index.js"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-es": "^1.4.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-unicorn": "^8.0.2",
    "eslint-plugin-vue": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "fisker/eslint-config-fisker",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-fisker": "^4.3.3",
    "esm": "^3.2.22",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "^0.15.0",
    "mem": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "prettier-config-fisker": "^1.0.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "bugs": {
    "url": "https://github.com/fisker/eslint-config-fisker/issues"
  },
  "homepage": "https://github.com/fisker/eslint-config-fisker#readme",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  }
}
