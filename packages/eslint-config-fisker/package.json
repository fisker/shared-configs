{
  "name": "@xwtec/eslint-config",
  "version": "2.1.2",
  "description": "xwtec eslint lint config.",
  "main": "index.js",
  "keywords": ["xwtec", "eslint", "config"],
  "files": [
    "index.js",
    "legacy.js",
    "vue.js",
    "rules/imports.js",
    "rules/index.js",
    "rules/prettier.js",
    "rules/vue.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "release": "np --no-yarn --no-cleanup",
    "prettier": "prettier **/*.{js,json,md} --write"
  },
  "license": "MIT",
  "dependencies": {
    "@vue/eslint-config-prettier": "^4.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": ["prettier --write", "git add"]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "xwtec/eslint-config",
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "np": "^4.0.1",
    "prettier": "^1.16.1"
  },
  "peerDependencies": {
    "eslint": "^5.12.1"
  }
}
