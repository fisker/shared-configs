{
  "name": "@fisker/eslint-config",
  "version": "1.6.6",
  "description": "fisker's Shareable ESLint Configuration",
  "keywords": [
    "config",
    "eslint",
    "fisker"
  ],
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "license": "MIT",
  "author": "fisker Cheung <lionkay@gmail.com> (https://www.fiskercheung.com/)",
  "main": "index.js",
  "files": [
    "plugins",
    "rules"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:docs": "node -r esm scripts/compare.js",
    "clean": "rimraf docs",
    "test": "ava"
  },
  "ava": {
    "babel": true,
    "files": [
      "test/test.js"
    ],
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-es": "3.0.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "17.0.1"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "ava": "3.5.0",
    "eslint": "6.8.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
