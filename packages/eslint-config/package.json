{
  "name": "@fisker/eslint-config",
  "version": "15.0.0",
  "description": "fisker's Shareable ESLint Configuration",
  "keywords": [
    "config",
    "eslint",
    "fisker"
  ],
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "license": "MIT",
  "author": "fisker Cheung <lionkay@gmail.com> (https://www.fiskercheung.com/)",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./*": "./*"
  },
  "files": [
    "index.js",
    "plugins",
    "ignores"
  ],
  "scripts": {
    "inspect": "eslint-config-inspector --config index.js",
    "test": "ava"
  },
  "ava": {
    "timeout": "2m",
    "verbose": true
  },
  "dependencies": {
    "@babel/core": "7.28.0",
    "@babel/eslint-parser": "7.28.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
    "@eslint/compat": "1.3.1",
    "@eslint/js": "9.32.0",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-xo": "0.48.0",
    "eslint-plugin-es-x": "9.0.0",
    "eslint-plugin-n": "17.21.3",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-regexp": "2.9.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "3.0.4",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-unicorn": "60.0.0",
    "globals": "16.3.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.1.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
