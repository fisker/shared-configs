{
  "name": "@fisker/eslint-config",
  "description": "fisker's Shareable ESLint Configuration",
  "license": "MIT",
  "author": "fisker Cheung <lionkay@gmail.com> (https://www.fiskercheung.com/)",
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "version": "1.2.1",
  "main": "index.js",
  "files": [
    "plugins",
    "rules"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:docs": "node -r esm scripts/compare.js",
    "clean": "rimraf docs",
    "test": "run-p test:*",
    "test:js": "eslint **/*.js --config ./index.js",
    "prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-unicorn": "9.1.1",
    "stylelint-prettier": "1.1.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "keywords": [
    "config",
    "eslint",
    "fisker"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
