{
  "name": "@fisker/eslint-config",
  "version": "1.4.0",
  "description": "fisker's Shareable ESLint Configuration",
  "keywords": [
    "config",
    "eslint",
    "fisker"
  ],
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "license": "MIT",
  "author": "fisker Cheung <lionkay@gmail.com> (https://www.fiskercheung.com/)",
  "files": [
    "plugins",
    "rules"
  ],
  "main": "index.js",
  "scripts": {
    "build": "run-p build:*",
    "build:docs": "node -r esm scripts/compare.js",
    "clean": "rimraf docs",
    "test": "ava"
  },
  "dependencies": {
    "babel-eslint": "10.0.3",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "14.0.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "eslint": "6.7.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "verbose": true,
    "helpers": [
      "**/fixtures/**"
    ]
  }
}
