{
  "name": "@fisker/eslint-config",
  "description": "fisker's Shareable ESLint Configuration",
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "version": "1.0.5",
  "main": "index.js",
  "author": {
    "email": "lionkay@gmail.com",
    "name": "fisker Cheung",
    "url": "https://www.fiskercheung.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "keywords": [
    "config",
    "eslint",
    "fisker"
  ],
  "dependencies": {
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-unicorn": "^8.0.2"
  },
  "files": [
    "index.js",
    "plugins",
    "rules"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:compare": "node -r esm scripts/compare.js",
    "clean": "rimraf docs",
    "prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write",
    "test": "run-p test:*",
    "test:js": "eslint **/*.js --config ./index.js"
  },
  "license": "MIT",
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
