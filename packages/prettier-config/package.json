{
  "name": "@fisker/prettier-config",
  "description": "fisker's Shareable prettier Configuration",
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "version": "1.0.6",
  "keywords": [
    "fisker",
    "prettier"
  ],
  "main": "lib/index.js",
  "author": {
    "email": "lionkay@gmail.com",
    "name": "fisker Cheung",
    "url": "https://www.fiskercheung.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "dependencies": {
    "@fisker/eslint-config": "^1.0.5"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "build:rules": "node -r esm ./scripts/generate-options.js && prettier readme.md --write",
    "clean": "rimraf lib",
    "dev": "rollup --config --watch",
    "test": "run-p test:*",
    "test:js": "ava"
  },
  "license": "MIT",
  "ava": {
    "require": "esm"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
