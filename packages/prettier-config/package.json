{
  "name": "@fisker/prettier-config",
  "description": "fisker's Shareable prettier Configuration",
  "version": "1.0.5",
  "main": "lib/index.js",
  "repository": "fisker/shared-configs",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {},
  "peerDependencies": {
    "prettier": "*"
  },
  "ava": {
    "require": "esm"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "build:rules": "node -r esm ./scripts/generate-options.js && prettier readme.md --write",
    "test": "run-p test:*",
    "test:js": "ava",
    "clean": "rimraf lib"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@fisker/eslint-config": "^1.0.4"
  }
}
