{
  "name": "@fisker/prettier-config",
  "description": "fisker's Shareable prettier Configuration",
  "version": "1.0.3",
  "main": "lib/index.js",
  "repository": "fisker/shared-configs",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "esm": "^3.2.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0"
  },
  "peerDependencies": {
    "prettier": "*"
  },
  "ava": {
    "require": "esm"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "run-p build:*",
    "build:js": "rollup --config",
    "build:rules": "node -r esm ./scripts/generate-options.js && prettier readme.md --write",
    "test": "run-p test:*",
    "test:js": "ava",
    "clean": "rimraf lib"
  },
  "files": [
    "lib"
  ]
}
