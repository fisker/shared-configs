{
  "name": "@fisker/renovate-config",
  "description": "fisker's Shareable Renovate Configuration",
  "license": "MIT",
  "author": "fisker Cheung <lionkay@gmail.com> (https://www.fiskercheung.com/)",
  "homepage": "https://github.com/fisker/shared-configs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fisker/shared-configs.git"
  },
  "bugs": {
    "url": "https://github.com/fisker/shared-configs/issues"
  },
  "version": "1.0.16",
  "files": [],
  "scripts": {
    "build": "yarn clean-package",
    "build-package": "node -r esm ./scripts/build.js",
    "clean-package": "node -r esm ./scripts/clean.js",
    "prepublish": "yarn build-package",
    "postpublish": "yarn clean-package",
    "pretest": "yarn build-package",
    "test": "ava",
    "posttest": "yarn clean-package"
  },
  "devDependencies": {
    "read-pkg": "^5.1.1",
    "write-pkg": "^4.0.0"
  },
  "keywords": [
    "fisker",
    "renovate"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "require": "esm"
  },
  "renovate-config": {
    "default": {
      "description": "Default Configuration",
      "extends": [
        ":enableRenovate",
        ":ignoreModulesAndTests",
        ":separateMajorReleases",
        ":combinePatchMinorReleases",
        ":renovatePrefix",
        ":semanticPrefixChore",
        ":disablePeerDependencies",
        ":disableDigestUpdates",
        ":semanticPrefixFixDepsChoreOthers",
        ":unpublishSafe",
        ":prImmediately",
        ":prHourlyLimitNone",
        ":disableRateLimiting",
        ":automergeRequireAllStatusChecks",
        ":maintainLockFilesDisabled",
        ":ignoreUnstable",
        ":respectLatest",
        ":updateNotScheduled",
        ":npm",
        ":automergeLinters",
        ":doNotPinPackage",
        ":semanticCommits",
        ":timezone(Asia/Shanghai)",
        ":automergePatch",
        ":automergePr"
      ],
      "ignorePresets": [],
      "packageRules": [
        {
          "depTypeList": [
            "devDependencies"
          ],
          "automerge": true
        }
      ],
      "rangeStrategy": "bump",
      "vulnerabilityAlerts": {
        "labels": [
          "security"
        ],
        "assignees": [
          "@fisker"
        ]
      }
    }
  }
}
