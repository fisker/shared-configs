    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "markdownlint-cli": "^0.15.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "prettier-config-fisker": "^1.0.0",
    "semantic-release": "^15.13.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/fisker/renovate-config-fisker/issues"
